{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://frankenjax.dev/schemas/fixture_manifest.v1.schema.json",
  "title": "FrankenJAX Fixture Manifest v1",
  "description": "Index of fixture bundles and oracle bindings for differential validation.",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "packet_id",
    "generated_at_unix_ms",
    "fixtures"
  ],
  "properties": {
    "schema_version": {
      "const": "frankenjax.fixture-manifest.v1"
    },
    "packet_id": {
      "type": "string",
      "minLength": 1
    },
    "generated_at_unix_ms": {
      "type": "integer",
      "minimum": 0
    },
    "fixtures": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "fixture_id",
          "family",
          "path",
          "oracle_source",
          "mode",
          "checksum_sha256",
          "expected_outcome"
        ],
        "properties": {
          "fixture_id": {
            "type": "string",
            "minLength": 1
          },
          "family": {
            "type": "string",
            "enum": [
              "jit",
              "grad",
              "vmap",
              "lax",
              "random",
              "dispatch",
              "cache",
              "runtime"
            ]
          },
          "path": {
            "type": "string",
            "minLength": 1
          },
          "oracle_source": {
            "type": "string",
            "minLength": 1
          },
          "mode": {
            "type": "string",
            "enum": [
              "strict",
              "hardened",
              "both"
            ]
          },
          "checksum_sha256": {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          "expected_outcome": {
            "type": "string",
            "enum": [
              "pass",
              "known-drift-allowlisted"
            ]
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            }
          }
        }
      }
    }
  }
}
